{
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.26",
    "core-js": "^3.38.1",
    "esbuild": "^0.23.1",
    "streams": "^2.0.0",
    "terser": "^5.32.0",
    "typescript": "^5.6.2"
  },
  "type": "commonjs",
  "scripts": {
    "build": "for f in *.js *.ts; do [[ $f != *.transpiled.js ]] && babel $f --out-file ${f%.*}.transpiled.js; done",
    "buildout": "babel --ignore '*.transpiled.js' -d transpiled/ *.js"
  }
}
